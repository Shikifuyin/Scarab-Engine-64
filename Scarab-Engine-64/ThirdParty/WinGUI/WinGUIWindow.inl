// WinGUIWindow implementation
inline WinGUIElementType WinGUIWindow::GetElementType() const {
	return WINGUI_ELEMENT_WINDOW;
}

inline UInt WinGUIWindow::GetChildCount() const {
	return m_iChildCount;
}
inline WinGUIElement * WinGUIWindow::GetChild( UInt iIndex ) const {
	DebugAssert( iIndex < m_iChildCount );
	return m_arrChildren[iIndex];
}

